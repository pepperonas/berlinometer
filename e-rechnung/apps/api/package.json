    "fastify": "^4.25.2",
    "form-data": "^4.0.4",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "minio": "^7.1.3",
    "nodemailer": "^6.9.8",
    "pdf-lib": "^1.17.1",
    "prisma": "^5.8.1",
    "puppeteer": "^21.11.0",
    "qrcode": "^1.5.3",
    "redis": "^4.6.12",
    "speakeasy": "^2.0.0",
    "uuid": "^9.0.1",
